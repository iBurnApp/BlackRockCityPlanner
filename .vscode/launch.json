    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Geocode Camps",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/cli/api.js",
            "args": ["-l", "${workspaceFolder}/../../data/2024/layouts/layout.json", "-f", "${workspaceFolder}/../../data/2024/APIData/camp.json", "-k", "location_string", "-o", "${workspaceFolder}/../../data/2024/APIData/camp-location.json"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Generate GeoJSON",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/cli/generate_all.js",
            "args": ["-d", "${workspaceFolder}/../../data/2024"]
        }
    ]
}